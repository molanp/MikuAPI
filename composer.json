{
    "name": "molanp/mikuapi",
    "description": "A flexible API with extensible plugin system.",
    "type": "project",
    "keywords": [
        "miku",
        "api",
        "templating"
    ],
    "homepage": "https://github.com/molanp/MikuAPI",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "molanp",
            "homepage": "https://github.com/molanp",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "topthink/framework": "^8.0",
        "topthink/think-orm": "^3.0",
        "topthink/think-filesystem": "^2.0",
        "topthink/think-view": "^2.0",
        "topthink/think-migration": "^3.1"
    },
    "require-dev": {
        "symfony/var-dumper": ">=4.2",
        "topthink/think-trace": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ],
        "test": "phpunit"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
